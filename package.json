{
  "name": "js-boilerplate",
  "version": "1.0.0",
  "description": "JavaScript development environment by @lomboboo",
  "scripts": {
    "start": "NODE_ENV='dev' npm-run-all --parallel clean-build security-check webpack-dev-server eslint:watch test:watch",
    "webpack-dev-server": "webpack-dev-server --config=./webpack.dev.config.js --inline",
    "security-check": "nsp check",
    "share": "lt --port 80 --subdomain lomboboo",
    "eslint": "esw webpack.config.* src build --color",
    "eslint:watch": "npm run eslint -- --watch",
    "test": "NODE_ENV='test' mocha --reporter progress ./mocha.setup.js \"./src/**/*.test.js\" ",
    "test:watch": "npm run test -- --watch",
    "clean-build": "rimraf ./build",
    "prebuild": "npm-run-all clean-build security-check eslint test",
    "build": "NODE_ENV='prod' webpack --config=./webpack.prod.config.js"
  },
  "author": "Roman Prokopchuk",
  "license": "MIT",
  "dependencies": {
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1"
  },
  "devDependencies": {
    "@types/jquery": "^2.0.40",
    "@types/lodash": "^4.14.63",
    "@types/node": "^7.0.13",
    "awesome-typescript-loader": "^3.0.8",
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "cheerio": "^0.22.0",
    "chunk-manifest-webpack-plugin": "^1.0.0",
    "compression": "1.6.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "eslint": "^3.14.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "^2.1.0",
    "handlebars": "^4.0.6",
    "handlebars-loader": "^1.4.0",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^9.9.1",
    "json-schema-faker": "0.3.6",
    "json-server": "0.8.22",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "localtunnel": "1.8.1",
    "mocha": "^3.2.0",
    "nock": "8.1.0",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "open": "0.0.5",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss-loader": "^1.2.2",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "typescript": "^2.2.1",
    "uglifyjs-webpack-plugin": "^0.3.0",
    "webpack": "2.2.1",
    "webpack-dev-server": "^1.16.3",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^4.0.0",
    "webpack-stable-module-id-and-hash": "^1.0.5"
  }
}
