{
  "name": "js-boilerplate",
  "version": "1.0.0",
  "description": "JavaScript development environment by @lomboboo",
  "scripts": {
    "start": "NODE_ENV='dev' npm-run-all --parallel clean-build security-check webpack-dev-server test",
    "webpack-dev-server": "webpack-dev-server --config=./config/webpack.dev.config.js --inline",
    "security-check": "nsp check",
    "share": "lt --port 80 --subdomain lomboboo",
    "test": "NODE_ENV='test' karma start karma.conf.js",
    "test:single": "NODE_ENV='test' karma start karma.conf.js --single-run",
    "clean-build": "rimraf ./build",
    "prebuild": "npm-run-all clean-build security-check test:single",
    "build": "NODE_ENV='prod' webpack --progress --config=./config/webpack.prod.config.js"
  },
  "author": "Roman Prokopchuk",
  "license": "MIT",
  "dependencies": {
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "rxjs": "^5.3.1"
  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/core-js": "^0.9.41",
    "@types/expect": "^1.20.1",
    "@types/jquery": "^2.0.40",
    "@types/lodash": "^4.14.63",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.13",
    "@types/rx": "^4.1.1",
    "awesome-typescript-loader": "^3.0.8",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "chunk-manifest-webpack-plugin": "^1.0.0",
    "compression": "1.6.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "ejs-loader": "^0.3.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "handlebars": "^4.0.6",
    "handlebars-loader": "^1.4.0",
    "html-webpack-plugin": "^2.28.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "karma": "^1.6.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript-preprocessor2": "^1.2.1",
    "karma-webpack": "^2.0.3",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "localtunnel": "1.8.1",
    "mocha": "^3.3.0",
    "nock": "8.1.0",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "open": "0.0.5",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-loader": "^1.2.2",
    "rimraf": "2.5.4",
    "sinon": "^2.1.0",
    "style-loader": "0.13.1",
    "tslint": "^5.1.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.2.1",
    "uglifyjs-webpack-plugin": "^0.3.0",
    "webpack": "2.2.1",
    "webpack-dev-server": "^1.16.3",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^4.0.0",
    "webpack-stable-module-id-and-hash": "^1.0.5"
  }
}
